<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="addBotox" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gaitlabapp.controllers.Wizards.AddBotoxController">
   <children>
      <Pane prefHeight="401.0" prefWidth="600.0">
         <children>
            <Label layoutX="20.0" layoutY="14.0" text="Botox Details: ">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Label layoutX="38.0" layoutY="53.0" text="Date" />
            <Label layoutX="18.0" layoutY="87.0" text="Body Location" />
            <Label layoutX="40.0" layoutY="121.0" text="Side " />
            <Label layoutX="43.0" layoutY="158.0" text="MD" />
            <Label layoutX="40.0" layoutY="191.0" text="Facility" />
            <Label layoutX="33.0" layoutY="228.0" text="Comments" />
            <TextField fx:id="date" layoutX="122.0" layoutY="49.0" />
            <TextField fx:id="bodyLocation" layoutX="122.0" layoutY="85.0" />
            <TextField fx:id="side" layoutX="122.0" layoutY="119.0" />
            <TextField fx:id="md" layoutX="121.0" layoutY="155.0" />
            <TextField fx:id="facility" layoutX="120.0" layoutY="189.0" />
            <TextField fx:id="comments" layoutX="116.0" layoutY="229.0" prefHeight="104.0" prefWidth="327.0" />
            <Button fx:id="saveButton" layoutX="300.0" layoutY="352.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="75.0" text="Save" onAction="#onSaveBotox"/>
            <Button fx:id="cancel" layoutX="378.0" layoutY="352.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="75.0" text="Cancel" onAction="#onCancel" />
         </children>
      </Pane>
   </children>
</AnchorPane>
