<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="addHealthCondition" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gaitlabapp.controllers.Wizards.AddHealthConditionController">
   <children>
      <Pane layoutX="-3.0" layoutY="-2.0" prefHeight="400.0" prefWidth="607.0">
         <children>
            <Label layoutX="33.0" layoutY="25.0" text="Health Condition Details">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Label layoutX="33.0" layoutY="80.0" text="Condition:" />
            <Label layoutX="45.0" layoutY="115.0" text="Age:" />
            <TextField fx:id="condition" layoutX="105.0" layoutY="76.0" />
            <TextField fx:id="age" layoutX="105.0" layoutY="110.0" />
            <Button fx:id="saveButton" layoutX="83.0" layoutY="161.0" mnemonicParsing="false" onAction="#onSaveHealth" prefHeight="25.0" prefWidth="78.0" text="Save" />
            <Button fx:id="cancelButton" layoutX="172.0" layoutY="161.0" mnemonicParsing="false" onAction="#onCancel" prefHeight="25.0" prefWidth="78.0" text="Cancel" />
         </children>
      </Pane>
   </children>
</AnchorPane>
