<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="botoxDetails" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gaitlabapp.controllers.Wizards.BotoxDetailsController">
   <children>
      <Pane layoutX="-2.0" prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label layoutX="14.0" layoutY="14.0" text="Botox Details">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <Label layoutX="30.0" layoutY="58.0" text="Date" />
            <TextField fx:id="botoxDate" layoutX="120.0" layoutY="54.0" />
            <Label layoutX="28.0" layoutY="94.0" text="Body Location" />
            <TextField fx:id="botoxLocation" layoutX="120.0" layoutY="90.0" />
            <Label layoutX="28.0" layoutY="126.0" text="Side" />
            <TextField fx:id="botoxSide" layoutX="120.0" layoutY="122.0" />
            <Label layoutX="28.0" layoutY="156.0" text="Provider" />
            <TextField fx:id="botoxProvider" layoutX="120.0" layoutY="152.0" />
            <Label layoutX="28.0" layoutY="190.0" text="Facility" />
            <TextField fx:id="botoxFacility" layoutX="119.0" layoutY="185.0" />
            <Button fx:id="saveButton" layoutX="135.0" layoutY="245.0" mnemonicParsing="false" text="Save Changes" />
            <Button fx:id="cancelButton" layoutX="234.0" layoutY="245.0" mnemonicParsing="false" text="Cancel" onAction="#onCancel" />
         </children>
      </Pane>
   </children>
</AnchorPane>
