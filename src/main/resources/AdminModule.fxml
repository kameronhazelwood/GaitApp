                     <TextArea layoutX="26.0" layoutY="71.0" prefHeight="349.0" prefWidth="240.0" />
                     <Label layoutX="26.0" layoutY="33.0" prefHeight="20.0" prefWidth="120.0" text="Appointment Types:">
                        <font>
