                           <Button layoutX="776.0" layoutY="439.0" mnemonicParsing="false" prefHeight="74.0" prefWidth="181.0" text="Delete Appointment Type Link" />
                        </children>
                     </Pane>
                  </children></AnchorPane>
           </content>
         </Tab>
     </tabs>
   </TabPane>
</AnchorPane>
