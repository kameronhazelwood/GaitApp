<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gaitlabapp.controllers.Wizards.AddOrthosisWizard">
    <children>
        <Pane prefHeight="281.0" prefWidth="540.0">
            <children>
                <Label layoutX="14.0" layoutY="63.0" text="Orthosis: ">
                    <font>
                        <Font name="System Bold" size="14.0" />
                    </font>
                </Label>
                <Button fx:id="saveButton" layoutX="300.0" layoutY="188.0" mnemonicParsing="false" onAction="#onSaveBotox" prefHeight="25.0" prefWidth="75.0" text="Save" />
                <Button fx:id="cancel" layoutX="401.0" layoutY="188.0" mnemonicParsing="false" onAction="#onCancel" prefHeight="25.0" prefWidth="75.0" text="Cancel" />
            <ComboBox fx:id="orthosisDropdown" layoutX="121.0" layoutY="61.0" prefHeight="20.0" prefWidth="288.0" />
            <Label layoutX="14.0" layoutY="136.0" text="Side:">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Label>
            <TextField fx:id="sideTextfield" layoutX="77.0" layoutY="134.0" />
            </children>
        </Pane>
    </children>

</AnchorPane>
